input               references.bib
output.file         references.bib

fmt.et.al           {.etal}

key.base            lower
key.format          {{%3#p(author){%1p(author)} # %2p(author)} . {%4d(year)}}
key.number.separator {}

pass.comments       on

print.align         14
print.align.string  18
print.align.key     0
print.indent        2
print.line.length   79
print.use.tab       off

preserve.key.case   on
